    "    \"    if not candidates:\\n\",\n",
    "    \"        raise RuntimeError(f\\\"No snapshots found under {root} for username={username}\\\")\\n\",\n",
    "    \"    return max(candidates, key=lambda p: p.stat().st_mtime)\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"snapshot_path = Path(snapshot_dir) if snapshot_dir else _pick_latest_snapshot_dir(snapshots_root, username)\\n\",\n",
