    "   \"source\": [\n",
    "    \"# Only consider BUY trades for binary up/down markets\\n\",\n",
    "    \"mask_ud_buy = (features[\\\"side\\\"] == \\\"BUY\\\") & (features[\\\"outcome\\\"].isin([\\\"Up\\\", \\\"Down\\\"]))\\n\",\n",
    "    \"\\n\",\n",
    "    \"tmp = features.loc[mask_ud_buy, [\\\"market_slug\\\", \\\"ts\\\", \\\"outcome\\\", \\\"size\\\"]].copy()\\n\",\n",
    "    \"tmp[\\\"bucket\\\"] = tmp[\\\"ts\\\"].dt.floor(f\\\"{int(complete_set_window_seconds)}s\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"by_bucket = (\\n\",\n",
    "    \"    tmp.pivot_table(\\n\",\n",
    "    \"        index=[\\\"market_slug\\\", \\\"bucket\\\"],\\n\",\n",
    "    \"        columns=\\\"outcome\\\",\\n\",\n",
    "    \"        values=\\\"size\\\",\\n\",\n",
    "    \"        aggfunc=\\\"sum\\\",\\n\",\n",
    "    \"        fill_value=0.0,\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"    .rename(columns={\\\"Up\\\": \\\"up_shares\\\", \\\"Down\\\": \\\"down_shares\\\"})\\n\",\n",
    "    \"    .reset_index()\\n\",\n",
    "    \")\\n\",\n",
    "    \"if \\\"up_shares\\\" not in by_bucket.columns:\\n\",\n",
    "    \"    by_bucket[\\\"up_shares\\\"] = 0.0\\n\",\n",
    "    \"if \\\"down_shares\\\" not in by_bucket.columns:\\n\",\n",
    "    \"    by_bucket[\\\"down_shares\\\"] = 0.0\\n\",\n",
    "    \"\\n\",\n",
    "    \"by_bucket[\\\"complete_set_flag\\\"] = (by_bucket[\\\"up_shares\\\"] > 0) & (by_bucket[\\\"down_shares\\\"] > 0)\\n\",\n",
    "    \"by_bucket[\\\"complete_set_shares\\\"] = np.minimum(by_bucket[\\\"up_shares\\\"], by_bucket[\\\"down_shares\\\"])\\n\",\n",
    "    \"\\n\",\n",
    "    \"features[\\\"bucket\\\"] = features[\\\"ts\\\"].dt.floor(f\\\"{int(complete_set_window_seconds)}s\\\")\\n\",\n",
    "    \"features = features.merge(\\n\",\n",
    "    \"    by_bucket[[\\\"market_slug\\\", \\\"bucket\\\", \\\"complete_set_flag\\\", \\\"complete_set_shares\\\"]],\\n\",\n",
    "    \"    how=\\\"left\\\",\\n\",\n",
    "    \"    on=[\\\"market_slug\\\", \\\"bucket\\\"],\\n\",\n",
    "    \")\\n\",\n",
    "    \"\\n\",\n",
    "    \"features[\\\"complete_set_flag\\\"] = features[\\\"complete_set_flag\\\"].fillna(False)\\n\",\n",
    "    \"features[\\\"complete_set_shares\\\"] = features[\\\"complete_set_shares\\\"].fillna(0.0)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Regime label (simple first pass)\\n\",\n",
    "    \"is_ud = features[\\\"outcome\\\"].isin([\\\"Up\\\", \\\"Down\\\"])\\n\",\n",
    "    \"is_buy = features[\\\"side\\\"] == \\\"BUY\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"features[\\\"regime\\\"] = \\\"OTHER\\\"\\n\",\n",
    "    \"features.loc[is_buy & is_ud, \\\"regime\\\"] = \\\"DIRECTIONAL\\\"\\n\",\n",
    "    \"features.loc[is_buy & is_ud & features[\\\"complete_set_flag\\\"], \\\"regime\\\"] = \\\"COMPLETE_SET_ARBITRAGE\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"features[\\\"regime\\\"].value_counts(dropna=False)\"\n",
    "   ]\n",
