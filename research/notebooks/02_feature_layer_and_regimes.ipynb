    "   \"source\": [\n",
    "    \"features = trades.copy()\\n\",\n",
    "    \"\\n\",\n",
    "    \"if market_trades.empty:\\n\",\n",
    "    \"    print(\\\"market_trades is empty; skipping market context features\\\")\\n\",\n",
    "    \"else:\\n\",\n",
    "    \"    market_trades_sorted = market_trades.sort_values([\\\"token_id\\\", \\\"ts\\\"], kind=\\\"stable\\\")\\n\",\n",
    "    \"    features = features.sort_values([\\\"token_id\\\", \\\"ts\\\"], kind=\\\"stable\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Allocate output columns\\n\",\n",
    "    \"    features[\\\"mt_last_price\\\"] = np.nan\\n\",\n",
    "    \"    features[\\\"mt_last_ts\\\"] = pd.NaT\\n\",\n",
    "    \"    features[\\\"mt_last_age_ms\\\"] = np.nan\\n\",\n",
    "    \"    for w in return_windows_seconds:\\n\",\n",
    "    \"        features[f\\\"mt_return_{w}s\\\"] = np.nan\\n\",\n",
    "    \"    features[f\\\"mt_trades_{flow_window_seconds}s\\\"] = np.nan\\n\",\n",
    "    \"    features[f\\\"mt_volume_{flow_window_seconds}s\\\"] = np.nan\\n\",\n",
    "    \"    features[f\\\"mt_notional_{flow_window_seconds}s\\\"] = np.nan\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Grouped searchsorted for efficiency (keeps it scalable for 10k-100k rows)\\n\",\n",
    "    \"    for token_id, mt in market_trades_sorted.groupby(\\\"token_id\\\", sort=False):\\n\",\n",
    "    \"        idx = features[\\\"token_id\\\"] == token_id\\n\",\n",
    "    \"        if not idx.any():\\n\",\n",
    "    \"            continue\\n\",\n",
    "    \"\\n\",\n",
    "    \"        mt_ts = mt[\\\"ts\\\"].to_numpy(dtype=\\\"datetime64[ns]\\\").astype(\\\"int64\\\")\\n\",\n",
    "    \"        mt_price = mt[\\\"price\\\"].to_numpy(dtype=\\\"float64\\\")\\n\",\n",
    "    \"        mt_size = mt[\\\"size\\\"].to_numpy(dtype=\\\"float64\\\")\\n\",\n",
    "    \"        mt_notional = (mt_price * mt_size).astype(\\\"float64\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        mt_cum_size = np.cumsum(mt_size)\\n\",\n",
    "    \"        mt_cum_notional = np.cumsum(mt_notional)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        tt_ts = features.loc[idx, \\\"ts\\\"].to_numpy(dtype=\\\"datetime64[ns]\\\").astype(\\\"int64\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # last observed market trade at/before user trade\\n\",\n",
    "    \"        end = np.searchsorted(mt_ts, tt_ts, side=\\\"right\\\")\\n\",\n",
    "    \"        has_last = end > 0\\n\",\n",
    "    \"        last_ix = np.maximum(end - 1, 0)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        last_price = np.where(has_last, mt_price[last_ix], np.nan)\\n\",\n",
    "    \"        last_ts = np.where(has_last, mt_ts[last_ix], np.datetime64(\\\"NaT\\\").astype(\\\"int64\\\"))\\n\",\n",
    "    \"\\n\",\n",
    "    \"        features.loc[idx, \\\"mt_last_price\\\"] = last_price\\n\",\n",
    "    \"        features.loc[idx, \\\"mt_last_ts\\\"] = pd.to_datetime(last_ts, utc=True, errors=\\\"coerce\\\")\\n\",\n",
    "    \"        features.loc[idx, \\\"mt_last_age_ms\\\"] = np.where(has_last, (tt_ts - last_ts) / 1_000_000.0, np.nan)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # returns over windows: last_price - price_at(t - window)\\n\",\n",
    "    \"        for w in return_windows_seconds:\\n\",\n",
    "    \"            w_ns = int(w * 1e9)\\n\",\n",
    "    \"            start = np.searchsorted(mt_ts, tt_ts - w_ns, side=\\\"right\\\")\\n\",\n",
    "    \"            has_start = start > 0\\n\",\n",
    "    \"            start_ix = np.maximum(start - 1, 0)\\n\",\n",
    "    \"            start_price = np.where(has_start, mt_price[start_ix], np.nan)\\n\",\n",
    "    \"            features.loc[idx, f\\\"mt_return_{w}s\\\"] = last_price - start_price\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # flow features for last N seconds\\n\",\n",
    "    \"        w = flow_window_seconds\\n\",\n",
    "    \"        w_ns = int(w * 1e9)\\n\",\n",
    "    \"        start = np.searchsorted(mt_ts, tt_ts - w_ns, side=\\\"right\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        cnt = end - start\\n\",\n",
    "    \"        vol = np.where(end > 0, mt_cum_size[last_ix], 0.0) - np.where(start > 0, mt_cum_size[start - 1], 0.0)\\n\",\n",
    "    \"        notional = np.where(end > 0, mt_cum_notional[last_ix], 0.0) - np.where(start > 0, mt_cum_notional[start - 1], 0.0)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        features.loc[idx, f\\\"mt_trades_{w}s\\\"] = cnt\\n\",\n",
    "    \"        features.loc[idx, f\\\"mt_volume_{w}s\\\"] = vol\\n\",\n",
    "    \"        features.loc[idx, f\\\"mt_notional_{w}s\\\"] = notional\\n\",\n",
    "    \"\\n\",\n",
    "    \"features[[\\\"ts\\\", \\\"token_id\\\", \\\"mt_last_price\\\", \\\"mt_return_30s\\\", \\\"mt_trades_60s\\\", \\\"mt_volume_60s\\\"]].head()\"\n",
    "   ]\n",
    "  },\n",
