    "   \"source\": [\n",
    "    \"from sklearn.compose import ColumnTransformer\\n\",\n",
    "    \"from sklearn.impute import SimpleImputer\\n\",\n",
    "    \"from sklearn.pipeline import Pipeline\\n\",\n",
    "    \"from sklearn.preprocessing import OneHotEncoder, StandardScaler\\n\",\n",
    "    \"from sklearn.cluster import KMeans\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Use only trades with TOB + market context for cleaner clustering.\\n\",\n",
    "    \"cluster_df = df.copy()\\n\",\n",
    "    \"cluster_df = cluster_df[cluster_df[\\\"tob_known\\\"] == 1]\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Some snapshots might not have market-trade features; include only existing cols.\\n\",\n",
    "    \"numeric_candidates = [\\n\",\n",
    "    \"    \\\"seconds_to_end\\\",\\n\",\n",
    "    \"    \\\"price\\\",\\n\",\n",
    "    \"    \\\"size\\\",\\n\",\n",
    "    \"    \\\"notional_usd\\\",\\n\",\n",
    "    \"    \\\"spread\\\",\\n\",\n",
    "    \"    \\\"tob_imbalance\\\",\\n\",\n",
    "    \"    \\\"edge_vs_mid\\\",\\n\",\n",
    "    \"    \\\"effective_spread_ratio\\\",\\n\",\n",
    "    \"    \\\"hour_utc\\\",\\n\",\n",
    "    \"    \\\"mt_last_age_ms\\\",\\n\",\n",
    "    \"    \\\"mt_return_30s\\\",\\n\",\n",
    "    \"    \\\"mt_return_300s\\\",\\n\",\n",
    "    \"    \\\"mt_trades_60s\\\",\\n\",\n",
    "    \"    \\\"mt_volume_60s\\\",\\n\",\n",
    "    \"    \\\"mt_notional_60s\\\",\\n\",\n",
    "    \"]\\n\",\n",
    "    \"numeric_features = [c for c in numeric_candidates if c in cluster_df.columns]\\n\",\n",
    "    \"\\n\",\n",
    "    \"categorical_candidates = [\\\"series\\\", \\\"asset\\\", \\\"outcome\\\", \\\"exec_type\\\", \\\"regime\\\"]\\n\",\n",
    "    \"categorical_features = [c for c in categorical_candidates if c in cluster_df.columns]\\n\",\n",
    "    \"\\n\",\n",
    "    \"preprocess = ColumnTransformer(\\n\",\n",
    "    \"    transformers=[\\n\",\n",
    "    \"        (\\n\",\n",
    "    \"            \\\"num\\\",\\n\",\n",
    "    \"            Pipeline(\\n\",\n",
    "    \"                steps=[\\n\",\n",
    "    \"                    (\\\"impute\\\", SimpleImputer(strategy=\\\"median\\\")),\\n\",\n",
    "    \"                    (\\\"scale\\\", StandardScaler()),\\n\",\n",
    "    \"                ]\\n\",\n",
    "    \"            ),\\n\",\n",
    "    \"            numeric_features,\\n\",\n",
    "    \"        ),\\n\",\n",
    "    \"        (\\n\",\n",
    "    \"            \\\"cat\\\",\\n\",\n",
    "    \"            Pipeline(\\n\",\n",
    "    \"                steps=[\\n\",\n",
    "    \"                    (\\\"impute\\\", SimpleImputer(strategy=\\\"most_frequent\\\")),\\n\",\n",
    "    \"                    (\\\"onehot\\\", OneHotEncoder(handle_unknown=\\\"ignore\\\")),\\n\",\n",
    "    \"                ]\\n\",\n",
    "    \"            ),\\n\",\n",
    "    \"            categorical_features,\\n\",\n",
    "    \"        ),\\n\",\n",
    "    \"    ]\\n\",\n",
    "    \")\\n\",\n",
    "    \"\\n\",\n",
    "    \"model = Pipeline(\\n\",\n",
    "    \"    steps=[\\n\",\n",
    "    \"        (\\\"preprocess\\\", preprocess),\\n\",\n",
    "    \"        (\\\"kmeans\\\", KMeans(n_clusters=int(k_clusters), n_init=10, random_state=7)),\\n\",\n",
    "    \"    ]\\n\",\n",
    "    \")\\n\",\n",
    "    \"\\n\",\n",
