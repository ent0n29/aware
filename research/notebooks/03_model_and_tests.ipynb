    "   \"source\": [\n",
    "    \"from sklearn.linear_model import LogisticRegression\\n\",\n",
    "    \"from sklearn.model_selection import TimeSeriesSplit\\n\",\n",
    "    \"from sklearn.metrics import accuracy_score, roc_auc_score\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"dir_df = df.copy()\\n\",\n",
    "    \"dir_df = dir_df[\\n\",\n",
    "    \"    (dir_df[\\\"side\\\"] == \\\"BUY\\\")\\n\",\n",
    "    \"    & (dir_df[\\\"outcome\\\"].isin([\\\"Up\\\", \\\"Down\\\"]))\\n\",\n",
    "    \"    & (dir_df[\\\"regime\\\"] == \\\"DIRECTIONAL\\\")\\n\",\n",
    "    \"    & (dir_df[\\\"series\\\"].isin([\\\"updown-15m\\\", \\\"up-or-down\\\"]))\\n\",\n",
    "    \"].copy()\\n\",\n",
    "    \"\\n\",\n",
    "    \"dir_df = dir_df.sort_values(\\\"ts\\\")\\n\",\n",
    "    \"y = (dir_df[\\\"outcome\\\"] == \\\"Up\\\").astype(int)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Feature set for this model: exclude outcome + realized pnl.\\n\",\n",
    "    \"drop_cols = {\\\"outcome\\\", \\\"realized_pnl\\\", \\\"resolved_outcome\\\", \\\"settle_price\\\", \\\"transaction_hash\\\", \\\"event_key\\\"}\\n\",\n",
    "    \"X = dir_df.drop(columns=[c for c in drop_cols if c in dir_df.columns])\\n\",\n",
    "    \"\\n\",\n",
    "    \"numeric_candidates = [\\n\",\n",
    "    \"    \\\"seconds_to_end\\\",\\n\",\n",
    "    \"    \\\"price\\\",\\n\",\n",
    "    \"    \\\"size\\\",\\n\",\n",
    "    \"    \\\"notional_usd\\\",\\n\",\n",
    "    \"    \\\"spread\\\",\\n\",\n",
    "    \"    \\\"tob_imbalance\\\",\\n\",\n",
    "    \"    \\\"edge_vs_mid\\\",\\n\",\n",
    "    \"    \\\"effective_spread_ratio\\\",\\n\",\n",
    "    \"    \\\"hour_utc\\\",\\n\",\n",
    "    \"    \\\"mt_last_age_ms\\\",\\n\",\n",
    "    \"    \\\"mt_return_30s\\\",\\n\",\n",
    "    \"    \\\"mt_return_300s\\\",\\n\",\n",
    "    \"    \\\"mt_trades_60s\\\",\\n\",\n",
    "    \"    \\\"mt_volume_60s\\\",\\n\",\n",
    "    \"    \\\"mt_notional_60s\\\",\\n\",\n",
    "    \"]\\n\",\n",
    "    \"num = [c for c in numeric_candidates if c in X.columns]\\n\",\n",
    "    \"\\n\",\n",
    "    \"cat_candidates = [\\\"series\\\", \\\"asset\\\", \\\"exec_type\\\"]\\n\",\n",
    "    \"cat = [c for c in cat_candidates if c in X.columns]\\n\",\n",
    "    \"\\n\",\n",
    "    \"pre = ColumnTransformer(\\n\",\n",
    "    \"    transformers=[\\n\",\n",
    "    \"        (\\n\",\n",
    "    \"            \\\"num\\\",\\n\",\n",
    "    \"            Pipeline([\\n\",\n",
    "    \"                (\\\"impute\\\", SimpleImputer(strategy=\\\"median\\\")),\\n\",\n",
    "    \"                (\\\"scale\\\", StandardScaler()),\\n\",\n",
    "    \"            ]),\\n\",\n",
    "    \"            num,\\n\",\n",
    "    \"        ),\\n\",\n",
    "    \"        (\\n\",\n",
    "    \"            \\\"cat\\\",\\n\",\n",
