    "   \"source\": [\n",
    "    \"from scipy.stats import mannwhitneyu\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"def bootstrap_mean_diff(a: np.ndarray, b: np.ndarray, *, iters: int = 50_000, seed: int = 7) -> dict:\\n\",\n",
    "    \"    rng = np.random.default_rng(seed)\\n\",\n",
    "    \"    a = a.astype(float)\\n\",\n",
    "    \"    b = b.astype(float)\\n\",\n",
    "    \"    a = a[np.isfinite(a)]\\n\",\n",
    "    \"    b = b[np.isfinite(b)]\\n\",\n",
    "    \"    if len(a) == 0 or len(b) == 0:\\n\",\n",
    "    \"        return {\\\"n_a\\\": len(a), \\\"n_b\\\": len(b)}\\n\",\n",
    "    \"\\n\",\n",
    "    \"    boot = []\\n\",\n",
    "    \"    for _ in range(iters):\\n\",\n",
    "    \"        aa = rng.choice(a, size=len(a), replace=True)\\n\",\n",
    "    \"        bb = rng.choice(b, size=len(b), replace=True)\\n\",\n",
    "    \"        boot.append(float(np.mean(aa) - np.mean(bb)))\\n\",\n",
    "    \"    boot = np.array(boot)\\n\",\n",
    "    \"    return {\\n\",\n",
    "    \"        \\\"n_a\\\": int(len(a)),\\n\",\n",
    "    \"        \\\"n_b\\\": int(len(b)),\\n\",\n",
    "    \"        \\\"mean_a\\\": float(np.mean(a)),\\n\",\n",
    "    \"        \\\"mean_b\\\": float(np.mean(b)),\\n\",\n",
    "    \"        \\\"mean_diff\\\": float(np.mean(a) - np.mean(b)),\\n\",\n",
    "    \"        \\\"ci_95\\\": [float(np.quantile(boot, 0.025)), float(np.quantile(boot, 0.975))],\\n\",\n",
    "    \"    }\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
